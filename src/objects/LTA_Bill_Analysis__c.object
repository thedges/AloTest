    <validationRules>
        <fullName>Fill_in_fields_before_Completion</fullName>
        <active>true</active>
        <description>Before the user updates Review Status to Completed, fill out all necessary fields.</description>
        <errorConditionFormula>AND(
    ISPICKVAL(Review_Status__c, &quot;Complete&quot;),
    OR
			   (LEN(Bill_Summary__c)=0,
       ISBLANK(TEXT(Operational_Impact__c)),
       ISBLANK(TEXT(Fiscal_Impact__c))
			   )
				)</errorConditionFormula>
        <errorMessage>Please fill out all sections before marking this Bill Analysis as Complete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fiscal_Impact_Required</fullName>
        <active>true</active>
        <description>This validation rule will ensure that if Fiscall Impact? = Yes, the Fiscal Impact field will be required.</description>
        <errorConditionFormula>AND( 
				   ISPICKVAL(Fiscal_Impact__c, &quot;Yes&quot;),
  		   LEN(Fiscal_Impact_Summary__c)=0
				)</errorConditionFormula>
        <errorMessage>Please define what the Fiscal Impact is before saving the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Operational_Impact_Required</fullName>
        <active>true</active>
        <description>This validation rule will ensure that if Operational Impact? = Yes, the Operational Impact field will be required.</description>
        <errorConditionFormula>AND( 
				ISPICKVAL(
								      Operational_Impact__c, &quot;Yes&quot;),
				LEN(Operational_Impact_Analysis__c)=0
